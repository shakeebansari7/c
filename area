#include <stdio.h>

   void main()
   {
    int age;
    float per;
    
    printf("enter your age ");
    scanf("%d",&age);
    printf("enter your per");
    scanf("%f",&per);
    if (age>=18 && per>=40)
        {
        printf("welcom");
        }
   
    else
        {
          printf("sorry,you are not eligibal");
        }
    
   }


